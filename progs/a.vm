: .start ; Program starts here
	
	ifeq eq, 0, 0
	print "Zero is equal to zero\n"

	ifeq lt, 5, 4
	print "5 is less than 4"
	
	mov reg, 0
	mov eax, 5
	: loop
		
		ifeq gt, $reg, $eax
		print "Reg (", $reg, ") is greater than Eax (", $eax, ")\n"
		
		add $reg, 1 ; Adds 1 to reg and pushes the result to the stack
		pop reg ; Gets the value back

		ifeq lt, $reg, 10
		jump loop

