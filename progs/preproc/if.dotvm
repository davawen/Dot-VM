#define _STD 1.0.0
#define PLATFORM Linux

: exit
	mov rax, 0
	syscall 1, 60
	jump ret

: main
	#if _STD def
		#if _STD eq 1.0.0
			#if PLATFORM eq Linux
				call exit
			#endif
			#if PLATFORM eq Windows
				print "Bruv"
			#endif
		#endif
		#if _STD ne 1.0.0
			call exit
		#endif
	#endif
	#if _STD ndef
		mov rax, 0
		syscall 1, 60
	#endif

